<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.davidleen29.tehui.acts.ApplyAcceptActivity">
    <com.davidleen29.tehui.widget.HeaderView android:background="#ccc" android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:exampleDimension="24sp" app:exampleColor="#33b5e5"
        app:headTitle="提交成功"
        android:id="@+id/header"

        />



    <ImageView
        android:id="@+id/apply_photo"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_apply_accept"
        android:layout_centerInParent="true"
        />
    <TextView
        android:layout_below="@+id/apply_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="请耐心等待审核..."
        android:textSize="@dimen/text_size_normal"
        android:textColor="@color/text_gray"
        />

</RelativeLayout>
